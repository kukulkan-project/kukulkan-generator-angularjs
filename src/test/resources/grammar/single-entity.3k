entity EntityTest {
    stringField : String required min(3) max(50),
    integerField : Integer required min(12) max(23),
    longField : Long required min(13) max(23),
    bigDecimalField : BigDecimal required min(15) max(23),
    floatField : Float required min(12) max(23),
    doubleField : Double required min(11) max(23),
    booleanField : Boolean required,
    localDateField : LocalDate required,
    zoneDateTimeField : ZonedDateTime required,
    blobField : Blob required min(5) max(5000000),
    anyBlobField : AnyBlob required min(78) max(5000000),
    imageBlobField : ImageBlob required min(21) max(5000000),
    textBlobField : TextBlob required min(3) max(5000000),
    instantField : Instant required
}
